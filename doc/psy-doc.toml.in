[library]
version="@version@"
browse_url = ""
repository_url = "git@github.com:UiL-OTS-labs/psy-lib.git"
website_url = ""
docs_url = ""
authors = "The UiL-OTS labs"
logo_url = ""
license = "MIT"
description = "The Psy Programming library"
devhelp=true
search_index = true

[theme]
name = "basic"
show_index_summary = true
show_class_hierarchy = true

dependencies = [
    "GLib-2.0",
    "GObject-2.0",
    "Gtk-4.0",
    "Pango-1.0"
]

[dependencies."GLib-2.0"]
name = "GLib"
description = "The base utility library"
doc_url = "https://docs.gtk.org/glib/"

[dependencies."GObject-2.0"]
name = "GObject"
description = "The base type system library"
doc_url = "https://docs.gtk.org/gobject/"
[dependencies."Gtk-4.0"]
name = "Gtk"
description = "The Gtk toolkit"
doc_url = "https://docs.gtk.org/gtk4/"
[dependencies."Pango-1.0"]
name = "Pango"
description = "Internationalized text layout and rendering"
doc_url = "https://docs.gtk.org/pango/"


[extra]
# A list of Markdown files; they will be parsed using the
# same rules as the documentation coming from the introspection
# data. The path of each file is relative to the content
# directory specified on the command line.
#
# The order in which they are included will be used when
# generating the index.
#
# The generated files will be placed in the root output directory
content_files = [
    "introduction.md",
    "drawing-with-psylib-developer.md",
]
# Additional images referenced by the documentation; their path
# is relative to the content directory specified on the command
# line.
#
# The image files will be copied into the root documentation,
# without replicating the directory structure in which they
# are listed.
content_images = [
]

urlmap_file = "urlmap.js"
